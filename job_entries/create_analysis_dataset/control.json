{
  "datasets": {
    "video_performance_metrics": {
      "dataset_name": "video_performance_metrics",
      "process_type": "g2i",
      "dependencies": {
        "r2g_datasets": [
          "trending_videos",
          "categories",
          "channels_basic_information"
        ]
      },
      "validations": {
        "join_keys": {
          "trending_videos.category_id": "categories.category_id",
          "trending_videos.channel_id": "channels_basic_information.channel_id"
        }
      }
    },
    "channel_engagement_metrics": {
      "dataset_name": "channel_engagement_metrics",
      "process_type": "g2i",
      "dependencies": {
        "r2g_datasets": ["channels", "countries", "trending_videos"]
      },
      "validations": {
        "join_keys": {
          "channels.country_id": "countries.country_id"
        }
      }
    },
    "category_popularity_metrics": {
      "dataset_name": "category_popularity_metrics",
      "process_type": "g2i",
      "dependencies": {
        "r2g_datasets": ["trending_videos", "categories"]
      },
      "validations": {
        "join_keys": {
          "trending_videos.category_id": "categories.category_id"
        }
      }
    },
    "author_activity_metrics": {
      "dataset_name": "author_activity_metrics",
      "process_type": "g2i",
      "dependencies": {
        "r2g_datasets": ["authors", "comment_threads", "replies"]
      },
      "validations": {
        "join_keys": {
          "comment_threads.author_id": "authors.author_id",
          "replies.author_id": "authors.author_id"
        }
      }
    },
    "related_videos_network": {
      "dataset_name": "related_videos_network",
      "process_type": "g2i",
      "dependencies": {
        "r2g_datasets": ["related_videos", "trending_videos"]
      },
      "validations": {
        "join_keys": {
          "related_video.video_id": "trending_videos.video_id"
        }
      }
    },
    "channel_growth_metrics": {
      "dataset_name": "channel_growth_metrics",
      "process_type": "g2i",
      "dependencies": {
        "r2g_datasets": ["channels"]
      },
      "validations": {
        "join_keys": {
          "channels.channel_id": "channels.channel_id"
        }
      }
    }
  }
}
